{
    "name": "steevanb/php-url-test",
    "license": "MIT",
    "type": "lib",
    "description": "Tests all urls of your application",
    "autoload": {
        "psr-4": {
            "steevanb\\PhpUrlTest\\": "src/"
        }
    },
    "require": {
        "php": "^7.1 | ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "steevanb/php-yaml": "^2.0",
        "symfony/console": "^3.2||^4.0||^5.0||^6.0",
        "symfony/options-resolver": "^3.2||^4.0||^5.0||^6.0",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/yaml": "^3.2||^4.0||^5.0||^6.0",
        "symfony/process": "^3.2||^4.0||^5.0||^6.0",
        "symfony/filesystem": "^3.2||^4.0||^5.0||^6.0"
    },
    "bin": ["bin/urltest"],
    "require-dev": {
        "symfony/var-dumper": "^4.0"
    }
}
